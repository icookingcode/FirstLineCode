<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.net.NetTechnologyActivity">

    <com.guc.firstlinecode.customviews.TitleLayout
        android:id="@+id/titleLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnWebview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="webview打开CSDN" />

    <Button
        android:id="@+id/btnHttpURLConnection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HttpURLConnection" />

    <Button
        android:id="@+id/btnXmlParse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="XML解析与json解析" />

    <Button
        android:id="@+id/btnRetrofit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Retrofit使用" />
</LinearLayout>